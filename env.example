# Discord Bot Config
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_BOT_CLIENT_ID=your_discord_application_id_here
DISCORD_GUILD_ID=your_discord_guild_id_here

# main base url (for reqbin)
DOMAIN=your-domain.com

# RequestBin service url
REQUESTBIN_SERVICE_URL=http://requestbin:3000

# RequestBin Cloudflare Tunnel token
REQBIN_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# RevShell Service config
REVSHELL_HTTP_BASE_URL=http://revshell:8000
REVSHELL_TCP_HOST=revshell
REVSHELL_TCP_PORT=3000
REVSHELL_HTTP_PORT=8000
REVSHELL_LISTEN_PORT=3000
REVSHELL_TCP_BIND_PORT=3000
REVSHELL_ACCESS_HOSTNAME=revshell.your-domain.com
REVSHELL_ACCESS_PORT=3000
REVSHELL_ACCESS_TLS=false
REVSHELL_AUTH_TIMEOUT_SECONDS=30
REVSHELL_HANDSHAKE_MAX_BYTES=2048

NODE_ENV=development
